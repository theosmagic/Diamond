{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "ES2020"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "./dist",
        "resolveJsonModule": true,
        "rootDir": "./",
        "skipLibCheck": true,
        "strict": true,
        "target": "ES2020"
    },
    "exclude": [
        "node_modules",
        "dist",
        "actions-1"
    ],
    "include": [
        "actions/**/*"
    ]
}